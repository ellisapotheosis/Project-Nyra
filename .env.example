# Nyra MCP Environment Variables
# Copy this to .env and fill in your values

# Core Services
GITHUB_TOKEN=your-github-token-here
FIRECRAWL_API_KEY=your-firecrawl-api-key-here

# Optional Services
NOTION_TOKEN=your-notion-integration-token-here
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your-qdrant-api-key-here

# Google Services (for Gemini/GenAI tools)
GOOGLE_API_KEY=your-google-api-key-here

# Infisical Secrets Management
INFISICAL_CLIENT_ID=your-infisical-client-id
INFISICAL_CLIENT_SECRET=your-infisical-client-secret

# Workspace Configuration
WORKSPACE_ROOT=C:/Users/edane/OneDrive/Documents/DevProjects/Project-Nyra
NYRA_SANDBOX=C:/NYRA/sandbox

# Feature Flags (set to false to enable)
ARCHON_DISABLED=false
MCP_USE_DISABLED=true
TOME_DISABLED=true
NOTION_DISABLED=true
RUV_DISABLED=true
GOFAST_DISABLED=true
ACTIVEPIECES_DISABLED=true
PULSE_FS_DISABLED=true

# MetaMCP Ports (don't change unless needed)
METAMCP_CORE_PORT=3000
METAMCP_HEAVY_PORT=3001
METAMCP_DEV_PORT=3002
METAMCP_OPTIONAL_PORT=3003